# -*- mode: snippet -*-
# name: rthen
# key: rthen
# --
.then(($1) => {
      res
        .status(200)
        .json({ message: '${1:$(upcase yas-text)} $0disponibles', $1: $1 });
      return next();
    })
    .catch((err) => {
      console.error(err.message);
      res
        .status(500)
        .json({ message: 'Error interno al listar los $1 disponibles.' });
      return next();
    });